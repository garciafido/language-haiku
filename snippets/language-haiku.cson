'.source.hk, .source.haiku':
  'Entity Definition':
    prefix: 'entity'
    body: """
      entity $1 {
        $0
      }
    """
    description: 'Entity Definition'
  'Workflow Definition':
    prefix: 'workflow'
    body: """
      workflow $1 {
        ${2:transitions}: Transitions {
            ${3:init}: Transition {
                from: $4
                to: $5
            }
            $0
        }
      }
    """
    description: 'Workflowty Definition'
  'Property':
    prefix: 'prop'
    body: """
      $1: $2
      $0
    """
    description: 'Property'
  'Reference property':
    prefix: 'ref'
    body: """
      $1: <ref>$2
      $0
    """
    description: 'Reference property'
  'Tabs':
    prefix: 'tabs'
    body: """
      ${1:tabs}: Tabs {
        ${2:main}: Tab {
            label: '${2/(.+)/\\u$1/}'
            $0
        }
      }
    """
    description: 'Tabs'
  'Tab':
    prefix: 'tab'
    body: """
        $1: Tab {
            label: '${1/(.+)/\\u$1/}'
            $0
        }
    """
    description: 'Tabs'

'.source.hk, .source.haiku':
  'Entity Definition':
    prefix: 'entity'
    body: """
      entity ${1:name} {
      }
    """
    description: 'Entity Definition'
  'Workflow Definition':
    prefix: 'workflow'
    body: """
      workflow ${1:name} {
        transitions: Transitions {
            init: Transition {
                from: ${2:from}
                to: ${3:from}
            }
        }
      }
    """
    description: 'Workflowty Definition'
  'Property':
    prefix: 'prop'
    body: """
      ${1:name} : ${2:type}
    """
    description: 'Property'
  'Reference property':
    prefix: 'ref'
    body: """
      ${1:name} : <ref>${2:type}
    """
    description: 'Reference property'
  'Tabs':
    prefix: 'tabs'
    body: """
      ${1:name} : Tabs {
        ${2:name} : Tab {
        }
      }
    """
    description: 'Tabs'
  'Tab':
    prefix: 'tab'
    body: """
        ${1:name} : Tab {
        }
    """
    description: 'Tabs'
